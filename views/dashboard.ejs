<h2>Your Booked Rooms</h2>

<% if (bookings.length === 0) { %>
  <p>You haven't booked any rooms yet.</p>
<% } else { %>
  <ul>
    <% bookings.forEach(booking => { %>
    <li>
  <strong><%= booking.listing?.title || "Unknown Room" %></strong><br>
  Check-in: <%= booking.checkIn ? booking.checkIn.toDateString() : "N/A" %><br>
  Check-out: <%= booking.checkOut ? booking.checkOut.toDateString() : "N/A" %><br>
  Adults: <%= booking.adults || 0 %>, Children: <%= booking.children || 0 %>
</li>

    <% }) %>
  </ul>
<% } %>
